{
    "name": "vscord",
    "displayName": "Discord Rich Presence",
    "version": "4.6.7",
    "description": "Fully customizable VS Code extension to get Discord Rich Presence integration",
    "main": "dist/extension.js",
    "author": "LeonardSSH <leonardssh22@gmail.com> (https://leonard.sh)",
    "contributors": [
        "xhayper <hayper1919@gmail.com>"
    ],
    "publisher": "LeonardSSH",
    "license": "MIT",
    "private": true,
    "icon": "assets/icon.png",
    "repository": {
        "type": "git",
        "url": "https://github.com/LeonardSSH/vscord.git"
    },
    "bugs": {
        "url": "https://github.com/LeonardSSH/vscord/issues"
    },
    "homepage": "https://github.com/LeonardSSH/vscord#readme",
    "keywords": [
        "discord",
        "vscode",
        "rich",
        "presence",
        "rich presence",
        "rpc",
        "game status"
    ],
    "galleryBanner": {
        "color": "#5865F2",
        "theme": "dark"
    },
    "activationEvents": [
        "onStartupFinished"
    ],
    "extensionKind": [
        "ui",
        "workspace"
    ],
    "categories": [
        "Visualization",
        "Other"
    ],
    "devDependencies": {
        "@types/file-size": "^1.0.1",
        "@types/git-url-parse": "^9.0.1",
        "@types/node": "16.x",
        "@types/vscode": "^1.72.0",
        "@xhayper/discord-rpc": "^1.0.12",
        "file-size": "^1.0.0",
        "git-url-parse": "^13.1.0",
        "husky": "^8.0.1",
        "prettier": "^2.7.1",
        "pretty-quick": "^3.1.3",
        "source-map-support": "^0.5.21",
        "tsup": "^6.3.0",
        "typescript": "^4.8.4"
    },
    "scripts": {
        "vscode:prepublish": "yarn run build",
        "build": "tsup",
        "watch": "tsup --watch",
        "lint": "prettier --check src/**/**",
        "format": "prettier --write src/**/*",
        "update": "yarn upgrade-interactive",
        "prepare": "husky install"
    },
    "engines": {
        "vscode": "^1.72.0"
    },
    "contributes": {
        "commands": [
            {
                "command": "vscord.enable",
                "title": "Enable Discord Presence",
                "category": "Discord Rich Presence"
            },
            {
                "command": "vscord.disable",
                "title": "Disable Discord Presence",
                "category": "Discord Rich Presence"
            },
            {
                "command": "vscord.workspace.enable",
                "title": "Enable Discord Presence in the current workspace",
                "category": "Discord Rich Presence"
            },
            {
                "command": "vscord.workspace.disable",
                "title": "Disable Discord Presence in the current workspace",
                "category": "Discord Rich Presence"
            },
            {
                "command": "vscord.disconnect",
                "title": "Disconnect Discord Presence from Discord",
                "category": "Discord Rich Presence"
            },
            {
                "command": "vscord.reconnect",
                "title": "Reconnect Discord Presence to Discord Gateway",
                "category": "Discord Rich Presence"
            }
        ],
        "configuration": {
            "title": "Discord Rich Presence",
            "type": "object",
            "properties": {
                "vscord.enabled": {
                    "type": "boolean",
                    "default": true
                },
                "vscord.app.id": {
                    "type": "string",
                    "default": "782685898163617802"
                },
                "vscord.app.name": {
                    "type": "string",
                    "enum": [
                        "Code",
                        "Visual Studio Code",
                        "VSCodium",
                        "Custom"
                    ],
                    "default": "Code"
                },
                "vscord.status.details.enabled": {
                    "type": "boolean",
                    "default": true
                },
                "vscord.status.details.idle.enabled": {
                    "type": "boolean",
                    "default": true
                },
                "vscord.status.details.text.idle": {
                    "type": "string",
                    "default": "Idling"
                },
                "vscord.status.details.text.editing": {
                    "type": "string",
                    "default": "In {workspace} {problems}"
                },
                "vscord.status.details.text.debugging": {
                    "type": "string",
                    "default": "Debugging {file_name}"
                },
                "vscord.status.details.text.viewing": {
                    "type": "string",
                    "default": "In {workspace} {problems}"
                },
                "vscord.status.state.enabled": {
                    "type": "boolean",
                    "default": true
                },
                "vscord.status.state.idle.enabled": {
                    "type": "boolean",
                    "default": false
                },
                "vscord.status.state.text.idle": {
                    "type": "string",
                    "default": ""
                },
                "vscord.status.state.text.editing": {
                    "type": "string",
                    "default": "Working on {file_name}:{current_line}:{current_column}"
                },
                "vscord.status.state.text.debugging": {
                    "type": "string",
                    "default": "Debugging: {workspace}"
                },
                "vscord.status.state.text.viewing": {
                    "type": "string",
                    "default": "Viewing {file_name}"
                },
                "vscord.status.state.text.noWorkspaceFound": {
                    "type": "string",
                    "default": "No workspace."
                },
                "vscord.status.image.large.key": {
                    "type": "string",
                    "default": "https://raw.githubusercontent.com/LeonardSSH/vscord/main/assets/icons/{lang}.png"
                },
                "vscord.status.image.large.text": {
                    "type": "string",
                    "default": "Editing a {lang} file"
                },
                "vscord.status.image.large.idle.key": {
                    "type": "string",
                    "default": "https://raw.githubusercontent.com/leonardssh/vscord/main/assets/icons/{idle_icon}.png"
                },
                "vscord.status.image.large.idle.text": {
                    "type": "string",
                    "default": "Idling"
                },
                "vscord.status.image.small.key": {
                    "type": "string",
                    "default": "https://raw.githubusercontent.com/LeonardSSH/vscord/main/assets/icons/{icon}.png"
                },
                "vscord.status.image.small.text": {
                    "type": "string",
                    "default": "{app_name}"
                },
                "vscord.status.image.small.idle.key": {
                    "type": "string",
                    "default": "{idle_icon}"
                },
                "vscord.status.image.small.idle.text": {
                    "type": "string",
                    "default": "zZz"
                },
                "vscord.status.image.small.debugging.key": {
                    "type": "string",
                    "default": "https://raw.githubusercontent.com/LeonardSSH/vscord/main/assets/icons/debugging.png"
                },
                "vscord.status.image.small.debugging.text": {
                    "type": "string",
                    "default": "{app_name}"
                },
                "vscord.status.showElapsedTime": {
                    "type": "boolean",
                    "default": true
                },
                "vscord.status.problems.enabled": {
                    "type": "boolean",
                    "default": true
                },
                "vscord.status.problems.text": {
                    "type": "string",
                    "default": "- {problemsCount} problems found"
                },
                "vscord.status.idle.check": {
                    "type": "boolean",
                    "default": true
                },
                "vscord.status.idle.disconnectOnIdle": {
                    "type": "boolean",
                    "default": false
                },
                "vscord.status.idle.resetElapsedTime": {
                    "type": "boolean",
                    "default": false
                },
                "vscord.status.idle.timeout": {
                    "type": "number",
                    "default": 300
                },
                "vscord.status.button.enabled": {
                    "type": "boolean",
                    "default": true
                },
                "vscord.status.button.active.enabled": {
                    "type": "boolean",
                    "default": true
                },
                "vscord.status.button.active.label": {
                    "type": "string",
                    "default": "View Repository"
                },
                "vscord.status.button.active.url": {
                    "type": "url",
                    "default": ""
                },
                "vscord.status.button.idle.enabled": {
                    "type": "boolean",
                    "default": false
                },
                "vscord.status.button.idle.label": {
                    "type": "string",
                    "default": ""
                },
                "vscord.status.button.idle.url": {
                    "type": "url",
                    "default": ""
                },
                "vscord.status.button.inactive.enabled": {
                    "type": "boolean",
                    "default": true
                },
                "vscord.status.button.inactive.label": {
                    "type": "string",
                    "default": null
                },
                "vscord.status.button.inactive.url": {
                    "type": "url",
                    "default": null
                },
                "vscord.ignore.workspaces": {
                    "type": "array",
                    "items": {
                        "type": "string"
                    },
                    "default": []
                },
                "vscord.ignore.workspacesText": {
                    "type": [
                        "string",
                        "object"
                    ]
                },
                "vscord.ignore.repositories": {
                    "type": "array",
                    "items": {
                        "type": "string"
                    },
                    "default": []
                },
                "vscord.ignore.organizations": {
                    "type": "array",
                    "items": {
                        "type": "string"
                    },
                    "default": []
                },
                "vscord.ignore.gitHosts": {
                    "type": "array",
                    "items": {
                        "type": "string"
                    },
                    "default": []
                },
                "vscord.file.size.humanReadable": {
                    "type": "boolean",
                    "default": true
                },
                "vscord.file.size.spec": {
                    "type": "string",
                    "enum": [
                        "si",
                        "iec",
                        "jedec"
                    ],
                    "default": "iec"
                },
                "vscord.file.size.fixed": {
                    "type": "number",
                    "default": 2
                },
                "vscord.file.size.spacer": {
                    "type": "string",
                    "default": " "
                },
                "vscord.behaviour.additionalFileMapping": {
                    "type": "object"
                },
                "vscord.behaviour.suppressNotifications": {
                    "type": "boolean",
                    "default": true
                },
                "vscord.behavoiur.prioritizeLanguagesOverExtensions": {
                    "type": "boolean",
                    "default": false
                },
                "vscord.behaviour.debug": {
                    "type": "boolean",
                    "default": false
                }
            }
        }
    },
    "packageManager": "yarn@3.2.4"
}
